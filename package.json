{
  "name": "res-suspenders",
  "version": "0.0.1",
  "scripts": {
    "start": "rescript && node src/index.mjs",
    "dev": "rescript build -w",
    "clean": "rescript clean",
    "build": "rm -rf build/ && rescript clean && rescript build && snowpack --config snowpack.prod.config.js build",
    "test": "rescript && mocha",
    "test-watch": "mocha --watch --parallel",
    "format": "rescript format src/**/*.res"
  },
  "pre-commit": [
    "format"
  ],
  "keywords": [
    "rescript"
  ],
  "author": "",
  "license": "LicenseRef-LICENSE.md",
  "devDependencies": {
    "chai": "^4.3.4",
    "mocha": "^9.1.1",
    "pre-commit": "^1.2.2"
  },
  "dependencies": {
    "rescript": "^9.1.2"
  }
}
